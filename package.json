{
  "name": "@epaolillo/react-crud",
  "version": "0.1.0",
  "type": "module",
  "description": "UI reusable en React",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.cjs",
      "types": "./dist/components/*.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["react", "crud", "admin", "ui", "components"],
  "author": "epaolillo",
  "license": "MIT"
}
  